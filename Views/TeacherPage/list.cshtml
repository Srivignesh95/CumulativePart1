@model IEnumerable<test1.Models.Teacher>
<h2 style="text-align:center;margin-bottom:2em;margin-top:2em;"> Teachers Details</h2>

<table class="table table-striped">
    <thead>
        <tr>
            <th scope="col">ID</th>
            <th scope="col">First Name</th>
            <th scope="col">First Name</th>
            <th scope="col"></th>
        </tr>
    </thead>
    <tbody>
@foreach (test1.Models.Teacher Teacher in Model)
{
            <tr>
                <th scope="row">@Teacher.Teacherid</th>
                <td>@Teacher.Teacherfname</td>
                <td>@Teacher.Teacherlname</td>
                <td> <a href="/TeacherPage/Show/@Teacher.Teacherid">Show More</a></td>
            </tr>
   /* <div>
        <a href="/TeacherPage/Show/@Teacher.Teacherid">@Teacher.Teacherfname @Teacher.Teacherlname</a>
    </div>*/
}
    </tbody>
</table>
